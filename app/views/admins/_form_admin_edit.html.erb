<%= form_for :admin, url: update_admin_path(@admin), method: :patch do |f| %>
  
  <div class="field">
    <%= f.label :name, 'имя' %><br/>
    <%= f.text_field :name, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :email, 'почта' %><br/>
    <%= f.email_field :email %>
  </div>

 
  <br/>
  <div class="actions">
    <%= f.submit "Сохранить", class: "btn btn-success" %>
  </div>
<% end %>

<%= @admin.inspect %>